<Window x:Class="Vms_page.LiveViewWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Live View" Height="600" Width="1000"
        Background="{DynamicResource BackgroundColor}" WindowStyle="SingleBorderWindow" ResizeMode="CanResize"
        WindowStartupLocation="CenterScreen">

         <Window.Resources>

         
         <LinearGradientBrush x:Key="CameraBackground" StartPoint="0,0" EndPoint="1,1">
             <GradientStop Color="#34495E" Offset="0"/>
             <GradientStop Color="#2C3E50" Offset="1"/>
