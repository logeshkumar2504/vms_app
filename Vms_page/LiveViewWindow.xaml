         <LinearGradientBrush x:Key="ControlPanelBackground" StartPoint="0,0" EndPoint="1,1">
             <GradientStop Color="#2C3E50" Offset="0"/>
             <GradientStop Color="#34495E" Offset="1"/>
         </LinearGradientBrush>
     </Window.Resources>

